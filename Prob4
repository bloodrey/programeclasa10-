n = 5
s1 = 0
i = 1
while i <= n:
    s1 += i
    i += 1
print("S1:", s1)


n = 5
s2 = 0
i = 1
while i <= n - 1:
    s2 += i * (i + 1)
    i += 1
print("S2:", s2)


n = 5
s3 = 0
i = 1
p = 1
while i <= n:
    p *= i
    s3 += p
    i += 1
print("S3:", s3)

n = 5
s4 = 0
i = 1
while i <= n:
    s4 += i ** 2
    i += 1
print("S4:", s4)


n = 5
s5 = 0
i = 1
while i <= n:
    s5 += i / (i + 1)
    i += 1
print("S5:", s5)
